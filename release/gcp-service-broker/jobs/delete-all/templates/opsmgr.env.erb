SCHEME=https
ADMIN_USER=<%= properties.cf.admin_user %>
ADMIN_PASSWORD=<%= properties.cf.admin_password %>
DOMAIN=<%= properties.domain %>
APP_DOMAIN=<%= properties.app_domains[0] %>
CF_ORG=<%= properties.org %>
CF_SPACE=<%= properties.space %>
CF_TARGET=https://api.<%= properties.app_domains[0] %>
CF_SKIP_SSL=<%= properties.ssl.skip_cert_verify %>
SECURITY_USER_NAME=<%= properties.security.user %>
SECURITY_USER_PASSWORD=<%= properties.security.password %>
APPLY_OPEN_SECURITY_GROUP=<%= properties.apply_open_security_group %>
ALLOW_PAID_SERVICE_PLANS=<%= properties.allow_paid_service_plans %>

<%
	plans = { }
	p("cloudsql_custom_plans").each do |plan|
		plan_name = plan['name']
		plans[plan_name] = plan
	end
%>
CLOUDSQL_CUSTOM_PLANS=<%= JSON.dump(plans) %>
<%
	plans = { }
	p("bigtable_custom_plans").each do |plan|
		plan_name = plan['name']
		plans[plan_name] = plan
	end
%>
BIGTABLE_CUSTOM_PLANS=<%= JSON.dump(plans) %>

ROOT_SERVICE_ACCOUNT_JSON=<%= properties.root_service_account_json %>
DB_HOST=<%= properties.db_host %>
DB_USERNAME=<%= properties.db_username %>
DB_PASSWORD=<%= properties.db_password %>
DB_PORT=<%= properties.db_port %>
CA_CERT=<%= properties.ca_cert %>
CLIENT_CERT=<%= properties.client_cert %>
CLIENT_KEY=<%= properties.client_key %>
